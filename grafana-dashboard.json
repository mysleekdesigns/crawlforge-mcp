{
  "dashboard": {
    "id": null,
    "title": "MCP WebScraper Monitoring Dashboard",
    "tags": ["mcp", "webscraper", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "red", "value": 0.9}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 1
          }
        },
        "targets": [
          {
            "expr": "mcp_health_status",
            "refId": "A",
            "legendFormat": "Overall Health"
          }
        ]
      },
      {
        "id": 2,
        "title": "Requests per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "targets": [
          {
            "expr": "rate(mcp_requests_total[5m])",
            "refId": "A",
            "legendFormat": "Total Requests/sec"
          },
          {
            "expr": "rate(mcp_errors_total[5m])",
            "refId": "B",
            "legendFormat": "Errors/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.02},
                {"color": "red", "value": 0.05}
              ]
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "rate(mcp_errors_total[5m]) / rate(mcp_requests_total[5m])",
            "refId": "A",
            "legendFormat": "Error Rate"
          }
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "mcp_cache_hits_total / (mcp_cache_hits_total + mcp_cache_misses_total)",
            "refId": "A",
            "legendFormat": "Hit Rate"
          }
        ]
      },
      {
        "id": 5,
        "title": "Response Time Percentiles",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(mcp_response_time_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.75, rate(mcp_response_time_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P75"
          },
          {
            "expr": "histogram_quantile(0.90, rate(mcp_response_time_bucket[5m]))",
            "refId": "C",
            "legendFormat": "P90"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mcp_response_time_bucket[5m]))",
            "refId": "D",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(mcp_response_time_bucket[5m]))",
            "refId": "E",
            "legendFormat": "P99"
          }
        ]
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "bytes"
          }
        },
        "targets": [
          {
            "expr": "mcp_memory_heap_used_bytes",
            "refId": "A",
            "legendFormat": "Heap Used"
          },
          {
            "expr": "mcp_memory_heap_total_bytes",
            "refId": "B",
            "legendFormat": "Heap Total"
          },
          {
            "expr": "mcp_memory_rss_bytes",
            "refId": "C",
            "legendFormat": "RSS"
          }
        ]
      },
      {
        "id": 7,
        "title": "Tool Usage Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "increase(mcp_tool_calls_total[1h])",
            "refId": "A",
            "legendFormat": "{{tool}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Tool Performance Comparison",
        "type": "table",
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "avg by (tool) (rate(mcp_tool_response_time_sum[5m]) / rate(mcp_tool_calls_total[5m]))",
            "refId": "A",
            "legendFormat": "{{tool}}",
            "format": "table"
          },
          {
            "expr": "rate(mcp_tool_calls_total[5m])",
            "refId": "B",
            "legendFormat": "{{tool}}",
            "format": "table"
          },
          {
            "expr": "rate(mcp_tool_errors_total[5m]) / rate(mcp_tool_calls_total[5m])",
            "refId": "C",
            "legendFormat": "{{tool}}",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Value #A": "Avg Response Time (ms)",
                "Value #B": "Requests/sec",
                "Value #C": "Error Rate",
                "tool": "Tool"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Worker Pool Status",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "mcp_worker_pool_active",
            "refId": "A",
            "legendFormat": "Active Workers"
          },
          {
            "expr": "mcp_worker_pool_queued",
            "refId": "B",
            "legendFormat": "Queued Tasks"
          },
          {
            "expr": "mcp_worker_pool_total",
            "refId": "C",
            "legendFormat": "Total Workers"
          }
        ]
      },
      {
        "id": 10,
        "title": "Connection Pool Status",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "mcp_connection_pool_active",
            "refId": "A",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "mcp_connection_pool_idle",
            "refId": "B",
            "legendFormat": "Idle Connections"
          },
          {
            "expr": "mcp_connection_pool_total",
            "refId": "C",
            "legendFormat": "Total Connections"
          }
        ]
      },
      {
        "id": 11,
        "title": "Top Error Types",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "topk(10, increase(mcp_errors_by_type_total[1h]))",
            "refId": "A",
            "legendFormat": "{{error_type}}",
            "format": "table"
          }
        ]
      },
      {
        "id": 12,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "rate(mcp_cache_hits_total[5m])",
            "refId": "A",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(mcp_cache_misses_total[5m])",
            "refId": "B",
            "legendFormat": "Cache Misses/sec"
          },
          {
            "expr": "rate(mcp_cache_writes_total[5m])",
            "refId": "C",
            "legendFormat": "Cache Writes/sec"
          },
          {
            "expr": "rate(mcp_cache_evictions_total[5m])",
            "refId": "D",
            "legendFormat": "Cache Evictions/sec"
          }
        ]
      },
      {
        "id": 13,
        "title": "Health Check Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 44},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short",
            "mappings": [
              {
                "options": {
                  "0": {"text": "Unhealthy"},
                  "1": {"text": "Healthy"}
                },
                "type": "value"
              }
            ]
          }
        },
        "targets": [
          {
            "expr": "mcp_health_check_status",
            "refId": "A",
            "legendFormat": "{{check_name}}",
            "format": "table"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": false,
          "iconColor": "rgba(0, 211, 255, 1)",
          "limit": 100,
          "showIn": 0,
          "tags": ["deployment"],
          "type": "tags"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "datasource": "prometheus",
          "query": "label_values(mcp_requests_total, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        },
        {
          "name": "tool",
          "type": "query",
          "datasource": "prometheus",
          "query": "label_values(mcp_tool_calls_total, tool)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        }
      ]
    },
    "links": [
      {
        "title": "MCP WebScraper Documentation",
        "url": "https://github.com/your-org/mcp-webscraper",
        "type": "link",
        "icon": "doc"
      }
    ]
  }
}