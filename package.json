{
  "name": "mcp-webscraper",
  "version": "3.0.0",
  "description": "Comprehensive MCP server for web scraping with 12 advanced tools including content extraction, document processing, search, crawling, and analysis capabilities",
  "main": "server.js",
  "bin": {
    "mcp-webscraper": "server.js",
    "mcp-webscraper-setup": "install.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development node server.js",
    "test": "node test-server.js",
    "test:performance": "node --expose-gc tests/performance/test-runner.js",
    "test:load": "node --expose-gc tests/performance/load-test.js",
    "test:memory": "node --expose-gc tests/performance/memory-test.js",
    "test:benchmark": "node --expose-gc tests/performance/benchmark.js",
    "test:integration": "node --expose-gc tests/integration/performance-integration.test.js",
    "test:performance:quick": "node tests/performance/test-runner.js",
    "test:security": "node tests/security/security-test-suite.js",
    "test:wave3": "node tests/validation/wave3-validation.js",
    "test:wave3:quick": "node tests/validation/wave3-validation.js --quick",
    "test:wave3:verbose": "node tests/validation/wave3-validation.js --verbose",
    "test:unit:wave3": "jest tests/unit/ResearchOrchestrator.test.js tests/unit/StealthMode.test.js tests/unit/LocalizationManager.test.js tests/unit/ChangeTracker.test.js",
    "test:integration:wave3": "jest tests/integration/wave3-integration.test.js",
    "test:all": "npm test && npm run test:integration && npm run test:performance:quick && npm run test:security && npm run test:wave3",
    "lint": "echo 'No linter configured yet'",
    "build": "echo 'No build step required for Node.js project'",
    "clean": "shx rm -rf cache/*.json logs/*.log performance-results/ test-results/",
    "docker:build": "docker build -t mcp-webscraper .",
    "docker:dev": "docker-compose up mcp-webscraper-dev",
    "docker:prod": "docker-compose up mcp-webscraper-prod",
    "docker:test": "docker-compose --profile testing up mcp-webscraper-test",
    "docker:perf": "docker-compose --profile performance up mcp-webscraper-perf",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "prepublishOnly": "npm run test:all && npm run clean"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "web-scraping",
    "scraper",
    "html-parser",
    "metadata-extraction",
    "link-extraction",
    "content-processing",
    "document-analysis",
    "web-crawler",
    "search-engine",
    "text-summarization",
    "content-analysis",
    "nlp",
    "ai-tools",
    "automation",
    "data-extraction",
    "pdf-processing",
    "sitemap-parser",
    "performance-optimized"
  ],
  "author": {
    "name": "MCP WebScraper Team",
    "email": "support@mcp-webscraper.com",
    "url": "https://github.com/mcp-webscraper"
  },
  "contributors": [
    {
      "name": "Development Team",
      "email": "dev@mcp-webscraper.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-webscraper/mcp-webscraper.git"
  },
  "bugs": {
    "url": "https://github.com/mcp-webscraper/mcp-webscraper/issues"
  },
  "homepage": "https://github.com/mcp-webscraper/mcp-webscraper#readme",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "files": [
    "server.js",
    "install.js",
    "src/",
    "README.md",
    "INSTALL.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    ".env.example"
  ],
  "dependencies": {
    "@googleapis/customsearch": "^5.0.1",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@mozilla/readability": "^0.6.0",
    "cheerio": "^1.1.2",
    "compromise": "^14.14.4",
    "diff": "^7.0.0",
    "dotenv": "^17.2.1",
    "franc": "^6.2.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^26.1.0",
    "lru-cache": "^11.1.0",
    "node-summarizer": "^1.0.7",
    "p-queue": "^8.1.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.54.2",
    "robots-parser": "^3.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "artillery": "^2.0.0",
    "clinic": "^13.0.0",
    "cross-env": "^10.0.0",
    "shx": "^0.4.0"
  }
}
